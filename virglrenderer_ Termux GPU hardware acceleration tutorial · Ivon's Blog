.badges-message .badges-message-icon { display: block; position: absolute; background: url("https://c.disquscdn.com/next/embed/assets/img/badges-message.b69d2b061f341fd85f0b2e8284c4b800.svg") center center / auto 60px no-repeat; width: 60px; min-height: 60px; top: 50%; right: 25px; margin-top: -30px; opacity: 0.35; }

.dark .badges-message .badges-message-icon { filter: invert(1); opacity: 0.25; }

.badges-message .comment-policy-text .comment-policy-link { margin: 8px 0px 4px; }

#tos__container { position: fixed; top: 10px; bottom: 0px; z-index: 1000; background: rgba(255, 255, 255, 0.7); transform: translate3d(0px, 0px, 0px); max-width: 100%; }

